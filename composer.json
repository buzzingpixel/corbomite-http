{
    "name": "buzzingpixel/corbomite-http",
    "description": "Corbomite HTTP",
    "version": "1.1.0",
    "license": "Apache-2.0",
    "homepage": "https://buzzingpixel.com/",
    "require": {
        "php": ">=7.2",
        "buzzingpixel/corbomite-di": "^1.0.1",
        "middlewares/fast-route": "^1.2",
        "franzl/whoops-middleware": "^1.0",
        "middlewares/request-handler": "^1.4",
        "zendframework/zend-diactoros": "^1.8",
        "zendframework/zend-httphandlerrunner": "^1.0",
        "relay/relay": "2.x@dev",
        "grafikart/psr15-csrf-middleware": "^1.0",
        "buzzingpixel/corbomite-config-collector": "^1.0.3"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^7.2",
        "squizlabs/php_codesniffer": "^3.4",
        "phpmd/phpmd": "^2.6",
        "buzzingpixel/corbomite-twig": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "corbomite\\http\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "corbomite\\tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true
    },
    "extra": {
        "diConfigFilePath": "src/diConfig.php",
        "twigExtensions": [
            "corbomite\\http\\HttpTwigExtension"
        ]
    }
}
